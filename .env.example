# Magic Pipeline Environment Configuration
# Copy this file to .env.local and update with your actual Supabase credentials

# Supabase Configuration
# Get these values from your Supabase project dashboard at https://supabase.com/dashboard
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Example values (replace with your actual values):
# VITE_SUPABASE_URL=https://abcdefghijklmnop.supabase.co
# VITE_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Application Configuration
VITE_APP_NAME="Magic Pipeline"
VITE_APP_VERSION="1.0.0"

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true

# Development Settings
VITE_LOG_LEVEL=info